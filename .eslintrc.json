{
  "extends": ["next/core-web-vitals", "next", "prettier"],
  "rules": {
    "quotes": [
      "error",
      "double"
    ],
    "semi": 2,
    "import/order": [
      "error",
      {
        "groups": [
          "external",
          "type",
          "index",
          "sibling",
          "parent",
          "internal",
          "builtin",
          "object"
        ]
      }
    ]
  }
}
